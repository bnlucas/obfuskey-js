{
  "name": "obfuskey",
  "version": "1.0.0",
  "description": "A robust obfuscation library for JavaScript/TypeScript.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier \"src/**/*.ts\" --check",
    "format:fix": "prettier \"src/**/*.ts\" --write"
  },
  "keywords": [
    "obfuscation",
    "security",
    "javascript",
    "typescript",
    "library"
  ],
  "author": "Nathan Lucas <bnlucas@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YourUsername/obfuskey-js.git"
  },
  "homepage": "https://github.com/YourUsername/obfuskey-js#readme",
  "bugs": {
    "url": "https://github.com/YourUsername/obfuskey-js/issues"
  },
  "devDependencies": {
    "@types/node": "^20.14.x",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "vitest": "^3.2.4"
  }
}
